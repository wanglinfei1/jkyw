<%if(data.length==0){%>
<li class="L_imgcor"><img src="../images/Docnull.png" alt=""></li>
<%}else{%>
<li class="clearfix frist">
    <span class="floatL span1">时间</span>
    <span class="floatL span2">高压（mmHg）</span>
    <span class="floatL span3">低压（mmHg）</span>
    <span class="floatL span4">心率（bpm）</span>
</li>
    <%data.myForEach(function(data,index){%>
    <li class="clearfix lsitR">
        <span class="floatL span1"><%=data.measureTime%></span>
        <%if(data.sbpRange=='up'){%>
        <span class="floatL span2 height"><em><%=data.sbp%><i></i></em></span>
        <%}else if(data.sbpRange=='down'){%>
        <span class="floatL span2 low"><em><%=data.sbp%><i></i></em></span>
        <%}else if(data.sbpRange=='normal'){%>
        <span class="floatL span2"><em><%=data.sbp%><i></i></em></span>
        <%}%>
        <%if(data.dbpRange=='up'){%>
        <span class="floatL span3 height"><em><%=data.dbp%><i></i></em></span>
        <%}else if(data.dbpRange=='down'){%>
        <span class="floatL span3 low"><em><%=data.dbp%><i></i></em></span>
        <%}else if(data.dbpRange=='normal'){%>
        <span class="floatL span3"><em><%=data.dbp%><i></i></em></span>
        <%}%>
        <%if(data.heartRateRange=='up'){%>
        <span class="floatL span4 height"><em><%=data.heartRate%><i></i></em></span>
        <%}else if(data.heartRateRange=='down'){%>
        <span class="floatL span4 low"><em><%=data.heartRate%><i></i></em></span>
        <%}else if(data.heartRateRange=='normal'){%>
        <span class="floatL span4"><em><%=data.heartRate%><i></i></em></span>
        <%}%>
    </li>
    <%})%>
<%}%>
